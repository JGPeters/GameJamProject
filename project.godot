MovementComponentBaseScript="*res://resources/Scripts/AutoLoads/movement_component_base_script.gd"
CollisionAttributeBaseScript="*res://resources/Scripts/AutoLoads/collision_attribute_base_script.gd"
SignalBusScript="*res://resources/Scripts/AutoLoads/signal_bus_script.gd"
SignalBus="*res://resources/Scripts/AutoLoads/signal_bus.tscn"
Enums="*res://resources/Scripts/AutoLoads/Enums.gd"
ComponentBaseScript="*res://resources/Scripts/AutoLoads/component_base_script.gd"

[display]

window/size/viewport_width=640
window/size/viewport_height=360
window/size/always_on_top=true
window/stretch/mode="viewport"
window/stretch/scale=2.0
window/stretch/scale_mode="integer"

[dotnet]

project/assembly_name="Echoes Gathering Game Jam"

[editor]

version_control/plugin_name="GitPlugin"
version_control/autoload_on_startup=true

[global_group]

